LETTER: 'letter'
UMINUS: '-'
DIGIT: 'digit'

list    = 
        |   list stat 

stat    =   expr
        |   LETTER '=' expr

expr    =  '(' expr ')'
        |   expr '*' expr
        |   expr '/' expr
        |   expr '+' expr
        |   expr '-' expr
        |   '-' expr UMINUS
        |   LETTER
        |   number

number  =   DIGIT  
        |   number DIGIT
        |   number '.' DIGIT

%%

%parsing start list
%mapping land LETTER